{
  "name": "@vlad-iakovlev/eslint-config",
  "version": "0.0.0",
  "description": "ESLint config for my projects",
  "homepage": "https://github.com/vlad-iakovlev/eslint-config#readme",
  "bugs": "https://github.com/vlad-iakovlev/eslint-config/issues",
  "repository": "https://github.com/vlad-iakovlev/eslint-config.git",
  "license": "MIT",
  "author": "Vladislav Iakovlev",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:ci": "DEBUG=eslint:eslint eslint .",
    "prettier": "prettier --write '**/*.(ts|js|md|json)'",
    "prettier:ci": "prettier --check '**/*.(ts|js|md|json)'"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript-eslint": "^8.51.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^22.19.3",
    "eslint": "^9.39.2",
    "grammy": "^1.39.2",
    "jiti": "^2.6.1",
    "lefthook": "^2.0.13",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "typescript": "^5.9.3"
  }
}
